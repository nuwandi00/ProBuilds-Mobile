    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="368dp"
        android:layout_height="520dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="-10dp"
        android:layout_marginEnd="22dp"

        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/Update"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewnew"
        app:srcCompat="@drawable/search_icon_background" />

    <TextView
        android:id="@+id/textViewnewq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="95dp"
        android:layout_marginEnd="98dp"
        android:text="Add Order"
        android:textColor="#231C1C"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2" />

    <TextView
        android:id="@+id/updateName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="248dp"
        android:shadowColor="#0B0B0B"
        android:text="StaffID"
        android:textColor="#3E3131"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textViewnewq" />

    <EditText
        android:id="@+id/docNameUpdate"
        android:layout_width="321dp"
        android:layout_height="53dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="22dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:hint="  EMPXXX"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/updateName" />

    <TextView
        android:id="@+id/SpecilitytextUpdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="220dp"
        android:shadowColor="#0B0B0B"
        android:text="Mateiral"
        android:textColor="#3E3131"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.231"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/docNameUpdate" />

    <EditText
        android:id="@+id/SpecilityUpdate"
        android:layout_width="321dp"
        android:layout_height="53dp"
        android:layout_marginStart="44dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:hint="  Rouble"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SpecilitytextUpdate" />

    <TextView
        android:id="@+id/AddressUpdatetext"
        android:layout_width="180dp"
        android:layout_height="16dp"
        android:layout_marginTop="16dp"
        android:shadowColor="#0B0B0B"
        android:text="Quantity"
        android:textColor="#3E3131"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/AddressUpdate"
        app:layout_constraintTop_toBottomOf="@+id/SpecilityUpdate" />

    <EditText
        android:id="@+id/AddressUpdate"
        android:layout_width="321dp"
        android:layout_height="53dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="12dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:hint="  3qubes"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AddressUpdatetext" />

    <TextView
        android:id="@+id/textViewUpdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="8dp"
        android:text="Enter required Date"
        android:textColor="#3E3131"
        android:textSize="15sp"
        android:textStyle="normal|bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AddressUpdate" />

    <EditText
        android:id="@+id/DateUpdate"
        android:layout_width="321dp"
        android:layout_height="53dp"
        android:layout_marginTop="8dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        android:ems="10"
        android:hint="Day/Month/Year"
        android:inputType="date"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/textViewUpdate"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView7Update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="8dp"
        android:text="Supplier"
        android:textColor="#3E3131"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/DateUpdate" />

    <TextView
        android:id="@+id/textView10Update"
        android:layout_width="48dp"
        android:layout_height="24dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="20dp"
        android:text="ID"
        android:textColor="#151212"
        android:textSize="15sp"
        android:textStyle="normal|bold"
        app:layout_constraintEnd_toStartOf="@+id/FromUpdate"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7Update" />

    <TextView
        android:id="@+id/textView11Update"
        android:layout_width="59dp"
        android:layout_height="23dp"
        android:layout_marginStart="61dp"
        android:layout_marginBottom="8dp"
        android:text="Name"
        android:textColor="#151212"
        android:textSize="15sp"
        android:textStyle="normal|bold"
        app:layout_constraintBottom_toTopOf="@+id/checkBox1"
        app:layout_constraintEnd_toStartOf="@+id/toUpdate"
        app:layout_constraintStart_toEndOf="@+id/FromUpdate" />

    <EditText
        android:id="@+id/FromUpdate"
        android:layout_width="74dp"
        android:layout_height="39dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="12dp"
        android:background="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/textView10Update"
        app:layout_constraintTop_toBottomOf="@+id/textView7Update"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/toUpdate"
        android:layout_width="75dp"
        android:layout_height="41dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="4dp"
        android:background="#FFFFFF"
        android:backgroundTint="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/textView11Update"
        app:layout_constraintTop_toBottomOf="@+id/textView7Update"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <CheckBox
        android:id="@+id/checkBox1"
        android:layout_width="304dp"
        android:layout_height="45dp"

        android:layout_marginEnd="40dp"
        android:layout_marginBottom="4dp"
        android:text="Hereby I confirm above details are correct"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        tools:ignore="TouchTargetSizeCheck" />

    <Button
        android:id="@+id/Update"
        android:layout_width="148dp"
        android:layout_height="47dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="#F6B917"
        android:text="Confirm"
        android:textColor="#131313"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.543"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textViewnew"
        android:layout_width="419dp"
        android:layout_height="65dp"
        android:layout_marginBottom="39dp"
        android:background="#FFCA1A"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="176dp"
        android:layout_marginBottom="20dp"
        android:text="Pro Builds"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textViewnew"
        app:layout_constraintEnd_toEndOf="@+id/textViewnew" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/textViewnew"
        app:layout_constraintStart_toStartOf="@+id/textViewnew"
        app:srcCompat="@drawable/baseline_arrow_back_24" />


</androidx.constraintlayout.widget.ConstraintLayout>